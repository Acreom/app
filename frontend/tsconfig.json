{
    "compilerOptions": {
        "target": "ES2018",
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"],
            "@/*": ["./*"]
        },
        "types": [
            "@types/node",
            "@nuxt/types",
            "@nuxtjs/axios",
            "@nuxtjs/sentry",
            "vue-final-modal",
            "@nuxtjs/vuetify",
            "./@types",
            "vue-property-decorator",
            "nuxt-webpack-optimisations",
        ]
    },
    "exclude": ["node_modules", ".nuxt", "dist", "android", "ios", "capacitor.config.ts"]
}
