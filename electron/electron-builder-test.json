{
  "productName": "acreom-test",
  "extraMetadata": {
    "name": "acreom-test"
  },
  "copyright": "Copyright Â© 2024 acreom",
  "appId": "com.acreom.acreom-desktop-test",
  "win": {
    "extraResources": [
      {
        "from": "./Assets/windows",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./build",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "directories": {
    "output": "dist-test",
    "buildResources": "./build",
    "app": "."
  },
  "mac": {
    "hardenedRuntime": true,
    "entitlements": "./notarize/entitlements.mac.plist",
    "extraResources": [
      {
        "from": "./Assets/mac-${arch}",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./build",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "linux": {
    "target": "AppImage",
    "category": "Office",
    "icon": "./build/icon.png",
    "extraResources": [
      {
        "from": "./Assets/linux",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./build",
        "to": "Assets",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "files": [
    "package.json",
    "**/*.js",
    "**/node_modules/**",
    "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
    "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
    "!**/node_modules/*.d.ts",
    "!**/node_modules/.bin",
    "!notarize"
  ],
  "protocols": {
    "name": "acreom",
    "schemes": [
      "acreom"
    ]
  }
}
