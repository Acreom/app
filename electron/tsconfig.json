{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "moduleResolution": "Node",
        "strict": false,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "paths": {
            "wdio-electron-service/*": ["./node_modules/wdio-electron-service/dist/*"],
            "@/*": ["./*"],
        },
        "types": ["node", "@wdio/globals/types", "mocha", "expect"],
        "typeRoots": ["./node_modules", "./node_modules/@types", "./../../node_modules/@types", "../../@types"]
    },
    "exclude": ["node_modules"]
}